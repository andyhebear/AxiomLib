<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <xs:element name="scene">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="sceneManagerOption"/>
        <xs:element minOccurs="0" ref="shadowSettings"/>
        <xs:element minOccurs="0" ref="resourceGroup"/>
        <xs:element minOccurs="0" ref="renderTargets"/>
        <xs:element minOccurs="0" ref="externals"/>
        <xs:element minOccurs="0" ref="environment"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="light"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="camera"/>
        <xs:element minOccurs="0" ref="nodes"/>
        <xs:element minOccurs="0" ref="viewports"/>
        <xs:element minOccurs="0" ref="animations"/>
        <xs:element minOccurs="0" ref="animationStates"/>
        <xs:element minOccurs="0" ref="terrain"/>
        <xs:element minOccurs="0" ref="octree"/>
        <xs:element minOccurs="0" ref="userDataReference"/>
      </xs:sequence>
      <xs:attributeGroup ref="attlist.scene"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.scene">
    <xs:attribute name="formatVersion" use="required"/>
    <xs:attribute name="id" type="xs:ID"/>
    <xs:attribute name="sceneManagerType"/>
    <xs:attribute name="sceneManagerName"/>
    <xs:attribute name="minOgreVersion"/>
    <xs:attribute name="author"/>
    <xs:attribute name="showBoundingBoxes" default="false">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="true"/>
          <xs:enumeration value="false"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:element name="sceneManagerOption">
    <xs:complexType>
      <xs:attributeGroup ref="attlist.sceneManagerOption"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.sceneManagerOption">
    <xs:attribute name="name" use="required"/>
    <xs:attribute name="type" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="bool"/>
          <xs:enumeration value="real"/>
          <xs:enumeration value="int"/>
          <xs:enumeration value="unsigned_int"/>
          <xs:enumeration value="short"/>
          <xs:enumeration value="unsigned_short"/>
          <xs:enumeration value="long"/>
          <xs:enumeration value="unsigned_long"/>
          <xs:enumeration value="size_t"/>
          <xs:enumeration value="string"/>
          <xs:enumeration value="vector3"/>
          <xs:enumeration value="matrix3"/>
          <xs:enumeration value="matrix4"/>
          <xs:enumeration value="quaternion"/>
          <xs:enumeration value="colourvalue"/>
          <xs:enumeration value="movableObject"/>
          <xs:enumeration value="camera"/>
          <xs:enumeration value="node"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="value" use="required"/>
  </xs:attributeGroup>
  <xs:element name="shadowSettings">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="colourDiffuse"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="shadowTextureConfig"/>
      </xs:sequence>
      <xs:attributeGroup ref="attlist.shadowSettings"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.shadowSettings">
    <xs:attribute name="shadowTechnique" default="none">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="none"/>
          <xs:enumeration value="stencilModulative"/>
          <xs:enumeration value="stencilAdditive"/>
          <xs:enumeration value="textureModulative"/>
          <xs:enumeration value="textureAdditive"/>
          <xs:enumeration value="textureModulativeIntegrated"/>
          <xs:enumeration value="textureAdditiveIntegrated"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="showDebugShadows" default="false">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="true"/>
          <xs:enumeration value="false"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="shadowFarDistance"/>
    <xs:attribute name="shadowDirectionalLightExtrusionDistance" default="10000"/>
    <xs:attribute name="shadowIndexBufferSize" default="51200"/>
    <xs:attribute name="shadowUseInfiniteFarPlane" default="true">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="true"/>
          <xs:enumeration value="false"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="shadowTextureCount"/>
    <xs:attribute name="shadowTextureSize"/>
    <xs:attribute name="shadowTexturePixelFormat">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="PF_UNKNOWN"/>
          <xs:enumeration value="PF_L8"/>
          <xs:enumeration value="PF_BYTE_L"/>
          <xs:enumeration value="PF_L16"/>
          <xs:enumeration value="PF_SHORT_L"/>
          <xs:enumeration value="PF_A8"/>
          <xs:enumeration value="PF_BYTE_A"/>
          <xs:enumeration value="PF_A4L4"/>
          <xs:enumeration value="PF_BYTE_LA"/>
          <xs:enumeration value="PF_R5G6B5"/>
          <xs:enumeration value="PF_B5G6R5"/>
          <xs:enumeration value="PF_R3G3B2"/>
          <xs:enumeration value="PF_A4R4G4B4"/>
          <xs:enumeration value="PF_A1R5G5B5"/>
          <xs:enumeration value="PF_R8G8B8"/>
          <xs:enumeration value="PF_B8G8R8"/>
          <xs:enumeration value="PF_A8R8G8B8"/>
          <xs:enumeration value="PF_A8B8G8R8"/>
          <xs:enumeration value="PF_B8G8R8A8"/>
          <xs:enumeration value="PF_R8G8B8A8"/>
          <xs:enumeration value="PF_X8R8G8B8"/>
          <xs:enumeration value="PF_X8B8G8R8"/>
          <xs:enumeration value="PF_BYTE_RGB"/>
          <xs:enumeration value="PF_BYTE_BGR"/>
          <xs:enumeration value="PF_BYTE_BGRA"/>
          <xs:enumeration value="PF_BYTE_RGBA"/>
          <xs:enumeration value="PF_A2R10G10B10"/>
          <xs:enumeration value="PF_A2B10G10R10"/>
          <xs:enumeration value="PF_DXT1"/>
          <xs:enumeration value="PF_DXT2"/>
          <xs:enumeration value="PF_DXT3"/>
          <xs:enumeration value="PF_DXT4"/>
          <xs:enumeration value="PF_DXT5"/>
          <xs:enumeration value="PF_FLOAT16_R"/>
          <xs:enumeration value="PF_FLOAT16_RGB"/>
          <xs:enumeration value="PF_FLOAT16_RGBA"/>
          <xs:enumeration value="PF_FLOAT32_R"/>
          <xs:enumeration value="PF_FLOAT32_RGB"/>
          <xs:enumeration value="PF_FLOAT32_RGBA"/>
          <xs:enumeration value="PF_FLOAT16_GR"/>
          <xs:enumeration value="PF_FLOAT32_GR"/>
          <xs:enumeration value="PF_DEPTH"/>
          <xs:enumeration value="PF_SHORT_RGBA"/>
          <xs:enumeration value="PF_SHORT_GR"/>
          <xs:enumeration value="PF_SHORT_RGB"/>
          <xs:enumeration value="PF_COUNT"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="shadowDirLightTextureOffset" default="0.6"/>
    <xs:attribute name="shadowTextureFadeStart" default="0.7"/>
    <xs:attribute name="shadowTextureFadeEnd" default="0.9"/>
    <xs:attribute name="shadowTextureSelfShadow">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="true"/>
          <xs:enumeration value="false"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="shadowTextureCasterMaterial"/>
    <xs:attribute name="shadowTextureReceiverMaterial"/>
    <xs:attribute name="shadowCasterRenderBackFaces" default="true">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="true"/>
          <xs:enumeration value="false"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:element name="shadowTextureConfig">
    <xs:complexType>
      <xs:attributeGroup ref="attlist.shadowTextureConfig"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.shadowTextureConfig">
    <xs:attribute name="shadowIndex" use="required"/>
    <xs:attribute name="width" use="required"/>
    <xs:attribute name="height" use="required"/>
    <xs:attribute name="pixelFormat" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="PF_UNKNOWN"/>
          <xs:enumeration value="PF_L8"/>
          <xs:enumeration value="PF_BYTE_L"/>
          <xs:enumeration value="PF_L16"/>
          <xs:enumeration value="PF_SHORT_L"/>
          <xs:enumeration value="PF_A8"/>
          <xs:enumeration value="PF_BYTE_A"/>
          <xs:enumeration value="PF_A4L4"/>
          <xs:enumeration value="PF_BYTE_LA"/>
          <xs:enumeration value="PF_R5G6B5"/>
          <xs:enumeration value="PF_B5G6R5"/>
          <xs:enumeration value="PF_R3G3B2"/>
          <xs:enumeration value="PF_A4R4G4B4"/>
          <xs:enumeration value="PF_A1R5G5B5"/>
          <xs:enumeration value="PF_R8G8B8"/>
          <xs:enumeration value="PF_B8G8R8"/>
          <xs:enumeration value="PF_A8R8G8B8"/>
          <xs:enumeration value="PF_A8B8G8R8"/>
          <xs:enumeration value="PF_B8G8R8A8"/>
          <xs:enumeration value="PF_R8G8B8A8"/>
          <xs:enumeration value="PF_X8R8G8B8"/>
          <xs:enumeration value="PF_X8B8G8R8"/>
          <xs:enumeration value="PF_BYTE_RGB"/>
          <xs:enumeration value="PF_BYTE_BGR"/>
          <xs:enumeration value="PF_BYTE_BGRA"/>
          <xs:enumeration value="PF_BYTE_RGBA"/>
          <xs:enumeration value="PF_A2R10G10B10"/>
          <xs:enumeration value="PF_A2B10G10R10"/>
          <xs:enumeration value="PF_DXT1"/>
          <xs:enumeration value="PF_DXT2"/>
          <xs:enumeration value="PF_DXT3"/>
          <xs:enumeration value="PF_DXT4"/>
          <xs:enumeration value="PF_DXT5"/>
          <xs:enumeration value="PF_FLOAT16_R"/>
          <xs:enumeration value="PF_FLOAT16_RGB"/>
          <xs:enumeration value="PF_FLOAT16_RGBA"/>
          <xs:enumeration value="PF_FLOAT32_R"/>
          <xs:enumeration value="PF_FLOAT32_RGB"/>
          <xs:enumeration value="PF_FLOAT32_RGBA"/>
          <xs:enumeration value="PF_FLOAT16_GR"/>
          <xs:enumeration value="PF_FLOAT32_GR"/>
          <xs:enumeration value="PF_DEPTH"/>
          <xs:enumeration value="PF_SHORT_RGBA"/>
          <xs:enumeration value="PF_SHORT_GR"/>
          <xs:enumeration value="PF_SHORT_RGB"/>
          <xs:enumeration value="PF_COUNT"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:element name="resourceGroup">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="resourceLocation"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="resourceDeclaration"/>
      </xs:sequence>
      <xs:attributeGroup ref="attlist.resourceGroup"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.resourceGroup">
    <xs:attribute name="name" use="required"/>
  </xs:attributeGroup>
  <xs:element name="resourceLocation">
    <xs:complexType>
      <xs:attributeGroup ref="attlist.resourceLocation"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.resourceLocation">
    <xs:attribute name="name" use="required"/>
    <xs:attribute name="type" use="required"/>
    <xs:attribute name="recursive" default="true">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="true"/>
          <xs:enumeration value="false"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="relativeTo" default="dotSceneFileLocation">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="dotSceneFileLocation"/>
          <xs:enumeration value="exeFileLocation"/>
          <xs:enumeration value="currentWorkingDir"/>
          <xs:enumeration value="absolute"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:element name="resourceDeclaration">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="NameValuePairList"/>
      </xs:sequence>
      <xs:attributeGroup ref="attlist.resourceDeclaration"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.resourceDeclaration">
    <xs:attribute name="name" use="required"/>
    <xs:attribute name="resourceType" use="required"/>
  </xs:attributeGroup>
  <xs:element name="NameValuePairList">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="NameValuePair"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="NameValuePair">
    <xs:complexType>
      <xs:attributeGroup ref="attlist.NameValuePair"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.NameValuePair">
    <xs:attribute name="name" use="required"/>
    <xs:attribute name="value" use="required"/>
  </xs:attributeGroup>
  <!--
    **************************************************************************************
    **************************************************************************************
    **************************************************************************************
  -->
  <xs:element name="renderTargets">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="renderWindow"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="renderTexture"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="multiRenderTarget"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="renderWindow">
    <xs:complexType>
      <xs:attributeGroup ref="attlist.renderWindow"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.renderWindow">
    <xs:attribute name="name" use="required"/>
    <xs:attribute name="width" use="required"/>
    <xs:attribute name="height" use="required"/>
    <xs:attribute name="fullscreen" default="false">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="true"/>
          <xs:enumeration value="false"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="priority"/>
    <xs:attribute name="active" default="true">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="true"/>
          <xs:enumeration value="false"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="autoUpdated" default="true">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="true"/>
          <xs:enumeration value="false"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="primary" default="true">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="true"/>
          <xs:enumeration value="false"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="visible" default="true">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="true"/>
          <xs:enumeration value="false"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="title"/>
    <xs:attribute name="colourDepth"/>
    <xs:attribute name="left"/>
    <xs:attribute name="top"/>
    <xs:attribute name="depthBuffer" default="true">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="true"/>
          <xs:enumeration value="false"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="FSAA" default="0"/>
    <xs:attribute name="displayFrequency"/>
    <xs:attribute name="vsync" default="false">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="true"/>
          <xs:enumeration value="false"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="border" default="resize">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="none"/>
          <xs:enumeration value="fixed"/>
          <xs:enumeration value="resize"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="outerDimensions" default="false">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="true"/>
          <xs:enumeration value="false"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:element name="renderTexture">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="NameValuePairList"/>
      </xs:sequence>
      <xs:attributeGroup ref="attlist.renderTexture"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.renderTexture">
    <xs:attribute name="name" use="required"/>
    <xs:attribute name="textureType" default="2d">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="1d"/>
          <xs:enumeration value="2d"/>
          <xs:enumeration value="3d"/>
          <xs:enumeration value="cubeMap"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="width" use="required"/>
    <xs:attribute name="height" use="required"/>
    <xs:attribute name="depth" use="required"/>
    <xs:attribute name="numberOfMipmaps"/>
    <xs:attribute name="pixelFormat" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="PF_UNKNOWN"/>
          <xs:enumeration value="PF_L8"/>
          <xs:enumeration value="PF_BYTE_L"/>
          <xs:enumeration value="PF_L16"/>
          <xs:enumeration value="PF_SHORT_L"/>
          <xs:enumeration value="PF_A8"/>
          <xs:enumeration value="PF_BYTE_A"/>
          <xs:enumeration value="PF_A4L4"/>
          <xs:enumeration value="PF_BYTE_LA"/>
          <xs:enumeration value="PF_R5G6B5"/>
          <xs:enumeration value="PF_B5G6R5"/>
          <xs:enumeration value="PF_R3G3B2"/>
          <xs:enumeration value="PF_A4R4G4B4"/>
          <xs:enumeration value="PF_A1R5G5B5"/>
          <xs:enumeration value="PF_R8G8B8"/>
          <xs:enumeration value="PF_B8G8R8"/>
          <xs:enumeration value="PF_A8R8G8B8"/>
          <xs:enumeration value="PF_A8B8G8R8"/>
          <xs:enumeration value="PF_B8G8R8A8"/>
          <xs:enumeration value="PF_R8G8B8A8"/>
          <xs:enumeration value="PF_X8R8G8B8"/>
          <xs:enumeration value="PF_X8B8G8R8"/>
          <xs:enumeration value="PF_BYTE_RGB"/>
          <xs:enumeration value="PF_BYTE_BGR"/>
          <xs:enumeration value="PF_BYTE_BGRA"/>
          <xs:enumeration value="PF_BYTE_RGBA"/>
          <xs:enumeration value="PF_A2R10G10B10"/>
          <xs:enumeration value="PF_A2B10G10R10"/>
          <xs:enumeration value="PF_DXT1"/>
          <xs:enumeration value="PF_DXT2"/>
          <xs:enumeration value="PF_DXT3"/>
          <xs:enumeration value="PF_DXT4"/>
          <xs:enumeration value="PF_DXT5"/>
          <xs:enumeration value="PF_FLOAT16_R"/>
          <xs:enumeration value="PF_FLOAT16_RGB"/>
          <xs:enumeration value="PF_FLOAT16_RGBA"/>
          <xs:enumeration value="PF_FLOAT32_R"/>
          <xs:enumeration value="PF_FLOAT32_RGB"/>
          <xs:enumeration value="PF_FLOAT32_RGBA"/>
          <xs:enumeration value="PF_FLOAT16_GR"/>
          <xs:enumeration value="PF_FLOAT32_GR"/>
          <xs:enumeration value="PF_DEPTH"/>
          <xs:enumeration value="PF_SHORT_RGBA"/>
          <xs:enumeration value="PF_SHORT_GR"/>
          <xs:enumeration value="PF_SHORT_RGB"/>
          <xs:enumeration value="PF_COUNT"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="usage" default="default">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="static"/>
          <xs:enumeration value="dynamic"/>
          <xs:enumeration value="writeOnly"/>
          <xs:enumeration value="staticWriteOnly"/>
          <xs:enumeration value="dynamicWriteOnly"/>
          <xs:enumeration value="dynamicWriteOnlyDiscardable"/>
          <xs:enumeration value="autoMipmap"/>
          <xs:enumeration value="rendertarget"/>
          <xs:enumeration value="default"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="priority"/>
    <xs:attribute name="active" default="true">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="true"/>
          <xs:enumeration value="false"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="autoUpdated" default="true">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="true"/>
          <xs:enumeration value="false"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:element name="multiRenderTarget">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="renderTargetAttachment"/>
      </xs:sequence>
      <xs:attributeGroup ref="attlist.multiRenderTarget"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.multiRenderTarget">
    <xs:attribute name="name" use="required"/>
  </xs:attributeGroup>
  <xs:element name="renderTargetAttachment">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="renderTexture"/>
      </xs:sequence>
      <xs:attributeGroup ref="attlist.renderTargetAttachment"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.renderTargetAttachment">
    <xs:attribute name="attachmentPoint" use="required"/>
  </xs:attributeGroup>
  <xs:element name="viewports">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="viewport"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="viewport">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="colourBackground"/>
      </xs:sequence>
      <xs:attributeGroup ref="attlist.viewport"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.viewport">
    <xs:attribute name="renderTargetName" use="required"/>
    <xs:attribute name="cameraName" use="required"/>
    <xs:attribute name="changeCameraAspectRatio" default="true">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="true"/>
          <xs:enumeration value="false"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="left" default="0"/>
    <xs:attribute name="top" default="0"/>
    <xs:attribute name="width" default="1"/>
    <xs:attribute name="height" default="1"/>
    <xs:attribute name="zOrder" default="0"/>
    <xs:attribute name="clearEveryFrame" default="true">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="true"/>
          <xs:enumeration value="false"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="clearBuffers" default="colour|depth"/>
    <xs:attribute name="materialSchemeName"/>
    <xs:attribute name="overlaysEnabled" default="true">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="true"/>
          <xs:enumeration value="false"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="skiesEnabled" default="true">
